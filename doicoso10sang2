#include<stdio.h>
#include<stdbool.h>
int top = -1;
int stack[100];
int max=100;
bool stackrong()
{
    if(top==-1)
     return true;
    return false;
}
bool stackday()
{
    if(top==max-1)
     return true;
    return false;
}
void in()
{
    printf("%d",stack[top]); 
}
void themdinh(int data)
{
    if(stackday()==true)
    printf("stack full");
    else
    {
        top=top+1;
        stack[top]=data;
    }
}
void xoadinh()
{
    if(stackrong()==true)
     printf("stack rong");
    else
    {
        top=top-1;
    }
}
void doicoso10sang2(int value)
{
    while(value>0)
    {
        themdinh(value%2);
        value=value/2;
    }
}
int main()
{
    int value;
    scanf("%d",&value);
    doicoso10sang2(value);
    do
    {
        in();
        xoadinh();
    }
    while(top!=-1);
 
}
