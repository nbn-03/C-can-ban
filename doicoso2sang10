#include<stdio.h>
#include<stdbool.h>
#include<math.h>
int top = -1;
int stack[100];
int max=100;
bool stackrong()
{
    if(top==-1)
     return true;
    return false;
}
bool stackday()
{
    if(top==max-1)
     return true;
    return false;
}
void in()
{
    printf("%d",stack[top]); 
}
void themdinh(int data)
{
    if(stackday()==true)
    printf("stack full");
    else
    {
        top=top+1;
        stack[top]=data;
    }
}
void xoadinh()
{
    if(stackrong()==true)
     printf("stack rong");
    else
    {
        top=top-1;
    }
}
void timgiaithua(int value)
{
    while(value>0)
    {
        themdinh(value%10);
        value=value/10;
    }
}
int kiemtra()
{
    int dem=0;
    for(int i=0;i<=top;i++)
    {
        if(stack[i]==1 || stack[i]==0)
            dem=dem+1;
            
    }
    return dem;
}
void chuyencoso2sang10(int value)
{
    int k=0;
        for(int i=0;i<=top;i++)
        {
            k=k+(stack[i]*pow(2,i));
        }
    printf("%d",k);
}
int main()
{
    int value;
    scanf("%d",&value);
    timgiaithua(value);
    if(kiemtra()==top+1)
    {
       chuyencoso2sang10(value); 
    }
    else
        printf("so ban nhap khong phai la co so 2");
}
