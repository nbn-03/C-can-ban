#include<stdio.h>
#include<stdbool.h>
#include<math.h>
int top = -1;
int stack[100];
int max=100;
bool stackrong()
{
    if(top==-1)
     return true;
    return false;
}
bool stackday()
{
    if(top==max-1)
     return true;
    return false;
}
void in()
{
    if(stack[top]==0)
       printf("%d",0);
    else if(stack[top]==1)
       printf("%d",1);
    else if(stack[top]==10)
       printf("%d",2);
    else if(stack[top]==11)
       printf("%d",3);
    else if(stack[top]==100)
       printf("%d",4);
    else if(stack[top]==101)
       printf("%d",5);
    else if(stack[top]==110)
       printf("%d",6);
    else if(stack[top]==111)
       printf("%d",7);
    else if(stack[top]==1000)
       printf("%d",8);
    else if(stack[top]==1001)
       printf("%d",9);
    else if(stack[top]==1010)
       printf("%c",65);
    else if(stack[top]==1011)
       printf("%c",66);
    else if(stack[top]==1100)
       printf("%c",67);
    else if(stack[top]==1101)
       printf("%c",68);
    else if(stack[top]==1110)
       printf("%c",69);
    else if(stack[top]==1111)
       printf("%c",70); 
}
void themdinh(int data)
{
    if(stackday()==true)
    printf("stack full");
    else
    {
        top=top+1;
        stack[top]=data;
    }
}
void xoadinh()
{
    if(stackrong()==true)
     printf("stack rong");
    else
    {
        top=top-1;
    }
}
void phahuystack()
{
    top=-1;
}
void timgiaithua(int value)
{
    while(value>0)
    {
        themdinh(value%10);
        value=value/10;
    }
}
int kiemtra()
{
    int dem=0;
    for(int i=0;i<=top;i++)
    {
        if(stack[i]==1 || stack[i]==0)
            dem=dem+1;
 
    }
    return dem;
}
void xacminh()
{
    if(kiemtra()==top+1)
    {
        phahuystack();
    }
    else
        printf("so ban nhap khong phai la co so 2");
}
void doicoso2sang16(int value)
{
    while(value>0)
    {
        themdinh(value%10000);
        value=value/10000;
    }
}
int main()
{
    int value;
    scanf("%d",&value);
    xacminh();
    doicoso2sang16(value);
    do
    {
        in();
        xoadinh();
    }
    while(top!=-1);
 
}
